 <table>
  <tr>
    <th><%= t("song.tracknumber") %></th>
    <th><%= t("song.artist") %></th>
    <th><%= t("song.album") %></th>
    <th><%= t("song.title") %></th>
    <th><%= t("song.length") %></th>
    <th><%= t("song.bitrate") %></th>
    <th><%= t("song.sample_rate") %></th>
    <!--<th><%#= t("song.filepath") %></th>-->
  </tr>
  <% iterate @songs do |song|%>
  <tr>
    <td><%= song.tracknumber %></td>
    <td><%= song.artist %></td>
    <td><%= song.album %></td>
    <td><a href="#<%=song.artist << " - " << song.title %>" class="play" filepath="<%= song.filepath %>"><%= song.title %></a></td>
    <td><%= song.length %></td>
    <td><%= song.bitrate %></td>
    <td><%= song.sample_rate %></td>
    <!--<td><%#= song.filepath %></td>-->
  </tr>
  <% end %>
</table>

